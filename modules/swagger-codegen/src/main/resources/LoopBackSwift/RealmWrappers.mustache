import Foundation
import RealmSwift

public class RealmString : Object {
    public dynamic var value:String? = nil
}

public class RealmInt : Object {
    public var value = RealmOptional<Int>(nil)
}

public class RealmFloat : Object {
    public var value = RealmOptional<Float>(nil)
}

public class RealmDouble : Object {
    public var value = RealmOptional<Double>(nil)
}

public class RealmBool : Object {
    public var value = RealmOptional<Bool>(nil)
}